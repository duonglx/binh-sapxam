entity GameInfo {
    gNo Long,
    gDatetime Instant,
    gDesc String,
    player1 String,
    player2 String,
    player3 String,
    player4 String,
    createdBy String,
    createdDate Instant
}

entity GameScore {
	gNo Long,
    playerScore1 Long,
    playerScore2 Long,
    playerScore3 Long,
    playerScore4 Long,
    createdDate Instant
}

relationship OneToMany {
  GameInfo to GameScore
}

paginate GameScore with pagination
    